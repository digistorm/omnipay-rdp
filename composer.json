{
    "name": "digistorm/omnipay-rdp",
    "type": "library",
    "description": "Red Dot Payment driver for the Omnipay payment processing library",
    "keywords": [
        "gateway",
        "red dot payment",
        "merchant",
        "omnipay",
        "pay",
        "payment"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": { "Omnipay\\Rdp\\" : "src/" }
    },
    "require": {
        "php": "^8.1",
        "omnipay/common": "^3",
        "symfony/http-client": "^7.1",
        "moneyphp/money": "^3.0"
    },
    "require-dev": {
        "omnipay/tests": "^4",
        "http-interop/http-factory-guzzle": "^1.2",
        "rector/rector": "^1.2",
        "phpstan/phpstan": "^1.12",
        "friendsofphp/php-cs-fixer": "^3.64",
        "spaze/phpstan-disallowed-calls": "^3.5"
    },
    "config": {
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "prefer-stable": true
}
